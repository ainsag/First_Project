<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#a0ab81"
    >


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Josparlayq"
        android:textColor="@color/black"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022"
        />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="410dp"
        android:layout_height="233dp"
        android:src="@drawable/a"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.028"
        />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="347dp"
        android:layout_height="29dp"
        android:clickable="true"
        android:onClick="career_b"
        android:text="Career and Business"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.454"
        android:drawableRight="@drawable/ic_add"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="347dp"
        android:layout_height="29dp"
        android:clickable="true"
        android:onClick="money"
        android:text="Money"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.495"
        android:drawableRight="@drawable/ic_add"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="347dp"
        android:layout_height="29dp"
        android:clickable="true"
        android:onClick="personal_growth"
        android:text="Personal_Growth"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.537"
        android:drawableRight="@drawable/ic_add"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="347dp"
        android:layout_height="29dp"
        android:clickable="true"
        android:onClick="health_sport"
        android:text="Health and Sport"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.578"
        android:drawableRight="@drawable/ic_add"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="347dp"
        android:layout_height="29dp"
        android:clickable="true"
        android:onClick="friends_environment"
        android:text="Friends and Environment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.619"
        android:drawableRight="@drawable/ic_add"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="347dp"
        android:layout_height="29dp"
        android:clickable="true"
        android:onClick="relations"
        android:text="Relations"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.66"
        android:drawableRight="@drawable/ic_add"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="347dp"
        android:layout_height="29dp"
        android:clickable="true"
        android:onClick="brightness_of_life"
        android:text="Brightness of Life"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.702"
        android:drawableRight="@drawable/ic_add"/>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="347dp"
        android:layout_height="29dp"
        android:clickable="true"
        android:onClick="creativity"
        android:text="Creativity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.743"
        android:drawableRight="@drawable/ic_add"/>


</androidx.constraintlayout.widget.ConstraintLayout>